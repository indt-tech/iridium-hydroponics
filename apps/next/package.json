{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-dev": "node prepare.js",
    "prod-standalone": "NODE_ENV=production node entrypoint.js",
    "dev-standalone": "FULLDEV=1 NODE_ENV=development node entrypoint.js",
    "prod-service": "NODE_ENV=production pm2 start pm2.config.js",
    "dev-service": "FULLDEV=1 NODE_ENV=development pm2 start pm2.config.js",
    "prod": "NODE_ENV=production pm2 start pm2.config.js --no-daemon",
    "dev": "FULLDEV=1 NODE_ENV=development pm2 start pm2.config.js --no-daemon",
    "build": "next build",
    "package": "yarn build",
    "lint": "next lint",
    "test": "yarn test:deps && jest",
    "test:deps": "npx playwright install && npx playwright install-deps"
  },
  "dependencies": {
    "@craftjs/core": "0.2.0-beta.8",
    "@craftjs/layers": "0.2.1",
    "@protocraft/core": "0.2.4-2",
    "@protocraft/layers": "0.2.1-2",
    "@react-three/drei": "9.77.3",
    "@react-three/fiber": "8.8.10",
    "@tamagui/config": "latest",
    "@tamagui/next-plugin": "latest",
    "@tamagui/next-theme": "latest",
    "app": "0.0.0",
    "compare-versions": "^3.4.0",
    "dotenv": "^16.3.1",
    "leva": "^0.9.35",
    "next": "^13.5.2",
    "playwright": "1.41.0",
    "protolib": "0.0.1",
    "raf": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.2.3",
    "react-dropzone-uploader": "2.11.0",
    "react-native": "^0.71.4",
    "react-native-web": "^0.18.12",
    "react-native-web-lite": "latest",
    "react-topics": "0.1.7",
    "three": "0.146.0"
  },
  "devDependencies": {
    "@jest/globals": "~29.7.0",
    "@types/jest": "~29.5.5",
    "@types/node": "20.6.0",
    "@types/react": "^18.0.27",
    "electron": "28.0.0",
    "electron-builder": "^24.9.1",
    "eslint-config-next": "^13.0.4",
    "jest": "~29.7.0",
    "pm2": "^5.3.0",
    "selenium-webdriver": "~4.16.0",
    "ts-jest": "~29.1.1"
  }
}
